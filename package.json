{
  "name": "nginxcentral",
  "version": "1.0.0",
  "description": "Central nginx ",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "postbuild": "mkdir -p build && cp package.json build/package.json && rm -rf build/html && cp -r html build/html && cd build && npm ci --only=production",
    "start": "node build/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q build/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SiegfriedSchmidt/NginxCentral.git"
  },
  "author": "Siegfried",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SiegfriedSchmidt/NginxCentral/issues"
  },
  "homepage": "https://github.com/SiegfriedSchmidt/NginxCentral#readme",
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-http-proxy": "^2.0.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/express-http-proxy": "^1.6.6",
    "@types/node": "^20.14.10",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "typescript": "^5.5.3"
  }
}
